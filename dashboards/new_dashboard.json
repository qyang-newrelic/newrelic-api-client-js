          {
            "nrql": "SELECT uniqueCount(entityId) from ComputeSample WHERE `ec2Tag_Name` LIKE'{{ec2Tag_Name}}%' and provider = 'Ec2Instance' facet `provider.ec2State` TIMESERIES SINCE 3 days ago LIMIT 1000"
